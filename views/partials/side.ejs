<style>
    .sideplace{
    z-index:100;
    width: 20vw;
    height: 100vw;
    background-color: #0f1a0f;
    position: fixed;
    /* left: 0;
    top: 0;
    padding: 0;
    margin: 0; */
    display: flex;
    flex-direction: column;
    overflow: hidden;
}
a{
    text-decoration: none;
    font-size: 1.2rem;
    font-weight: 500;
    color: rgb(215, 243, 216);
}
.sideplace ul{
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    list-style: none;
}
.sideplace li{
    width: 90%;
    padding:15px 20px;
    border-radius: 10px ;
    font-size: 1.2rem;
    font-weight: 500;
    cursor: pointer;
    transition: all 0.3s ease-in-out;
}
.sideplace li:hover{
    background-color: rgb(215, 243, 216);
}
.sideplace li.active{
    background-color: rgb(215, 243, 216);
}
.sideplace li a{
    position: relative;
}
li.active a, li:hover a{
    color: #223a22;
}
li.active svg, li:hover svg{
    fill: #223a22;
}
li.active .list .proIco, li:hover .list .proIco{
    fill: #223a22;
}
.profile-pic{
    width: 100px;
    height: 100px;
    border-radius: 50%;
    background-color: rgb(215, 243, 216);
    border: 4px solid #111b11;
    align-self: center;
    margin: 50px;
}
.profile-pic svg{
    fill: rgb(123, 212, 130);
}
li svg, .proIco{
    position: relative;
    top: 20%;
    fill: rgb(215, 243, 216);
    height: 24px;
    width: 24px;
}
.pop{
    position: absolute;
    top: -2%;
    right: -10%;
    background-color: rgb(132, 255, 121);
    border-radius: 50%;
    height: 15px;
    width: 15px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #242121;
}
.pop p{
    font-weight: 700;
    font-size: 10px;
}
</style>
